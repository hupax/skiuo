# ===============================================
# StreamMind Environment Configuration
# ===============================================
# Copy this file to .env and configure your values
# DO NOT commit .env to version control
# ===============================================

# ===================
# REQUIRED - Qwen API
# ===================
# Get your API key from: https://dashscope.console.aliyun.com/
QWEN_API_KEY=sk-your-qwen-api-key-here
QWEN_API_URL=https://dashscope.aliyuncs.com/api/v1/services/aigc/multimodal-generation/generation

# ===================
# REQUIRED - Minio Object Storage
# ===================
# Used for uploading video windows to Minio for Qwen video analysis
# Minio is S3-compatible object storage
MINIO_ENDPOINT=https://minio-api.supanx.net
MINIO_BUCKET=test
MINIO_ACCESS_KEY=your-minio-access-key
MINIO_SECRET_KEY=your-minio-secret-key
MINIO_PUBLIC_URL=https://minio-api.supanx.net/test/

# ===================
# REQUIRED - Security
# ===================
# Generate with: openssl rand -base64 32
JWT_SECRET=change-me-to-a-secure-random-string-at-least-32-characters

# Generate with: openssl rand -base64 16
POSTGRES_PASSWORD=change-me-to-a-secure-password

# ===================
# Database (PostgreSQL)
# ===================
POSTGRES_DB=streammind
POSTGRES_USER=streammind

# ===================
# Service Ports (Optional)
# ===================
# Defaults are fine for most cases
CORE_SERVICE_PORT=8080
GRPC_PORT=9090
AI_SERVICE_PORT=8000

# ===================
# Logging
# ===================
# Options: DEBUG, INFO, WARN, ERROR
LOG_LEVEL=INFO

# ===================
# Spring Boot Profile
# ===================
# Options: dev, prod
SPRING_PROFILES_ACTIVE=prod

# ===================
# JVM Options (Optional)
# ===================
# Override default JVM settings
# JAVA_OPTS=-Xms512m -Xmx1024m
