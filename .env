# ===================
# Core Service (Spring Boot)
# ===================
SPRING_DATASOURCE_URL=jdbc:postgresql://postgres:5432/streammind
SPRING_DATASOURCE_USERNAME=streammind
SPRING_DATASOURCE_PASSWORD=streammind_password_change_me

SPRING_REDIS_HOST=redis
SPRING_REDIS_PORT=6379

